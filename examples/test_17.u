int_32 i;
int_32 n = 5;
int_32 p;

int_32 prime[6];

while(i < n + 1)
{
    prime[i] = 1;
    i = i + 1;
}

p = 2;

while(p * p < n + 1)
{
    if(prime[p] == 1)
    {
        i = p * 2;

        while(i < n + 1)
        {
            prime[i] = 0;
            i = i + p;
        }
        
    }

    p = p + 1;
}

i = 2;
while(i < n + 1)
{
    if(prime[i] == 1)
    {
        @print i;
    }
    i = i + 1;
}

@print "done\n";