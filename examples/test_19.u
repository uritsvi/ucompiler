void main()
{
    int_32 i;
    int_32 j;
    int_32 range = 10;
    int_32 min;

    int_32 A[10];
    int_32 temp;

    while (i < range)
    {
        A[i] = range - i;

        i = i + 1;
    }

    while (j < range - 1)
    {
        min = j;
        i = j + 1;
        while (j < range)
        {
            if (A[i] < A[min])
            {
                min = i;
                @print min;
            }
            j = j + 1;
        }

        j = j + 1;
    }

    @assert i, 1;
    @assert j, 11;
    @assert range, 10;
    @assert min, 1;
    @assert temp, 0;
}